# Mixpanel Configuration
MIXPANEL_TOKEN=your_mixpanel_token
MIXPANEL_PROJECT_ID=your_mixpanel_project_id

# Firebase Configuration - Enable for production
USE_FIREBASE=true
USE_MOCK_DATA=false
